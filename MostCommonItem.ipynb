   "source": [
    "def get_best_item(recommended_items):\n",
    "    max_rank = recommended_items[0][1]\n",
    "    max_rank_items = []\n",
    "    for items,rank in recommended_items:\n",
    "        if(rank == max_rank):\n",
    "            max_rank_items += list(items)\n",
    "    c = Counter(max_rank_items)\n",
    "    return c.most_common(1)[0][0]"
   ]
  }
